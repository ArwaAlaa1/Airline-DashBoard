@model AirLine.ViewModel.AirLineCompanyModel

@{
    ViewData["Title"] = "Create";
}
<h1>Create</h1>

<h4>AirLineModel</h4>
<hr />
<!-- Vertical Form -->
<form class="row g-3" asp-action="Create">
    <div class="col-12">
        <label for="inputNanme4" class="form-label">Id</label>
        <input asp-for="Id" class="form-control" id="inputNanme4">
    </div>
    <div class="col-12">
        <label for="inputEmail4" class="form-label">Name Of AirLine</label>
        <input asp-for="Name" class="form-control" id="inputEmail4">
    </div>
    <div class="col-12">
        <label for="inputPassword4" class="form-label">Contact Name</label>
        <input asp-for="Contact_Name" class="form-control" id="inputPassword4">
    </div>
    <div class="col-12">
        <label for="inputAddress" class="form-label">Address</label>
        <input asp-for="Address" class="form-control" id="inputAddress" placeholder="City">
    </div>
    <div class="text-center">
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
</form><!-- Vertical Form -->
<!-- Vendor JS Files -->
<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
